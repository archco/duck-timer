{
  "name": "duck-timer",
  "version": "0.4.6",
  "description": "The helpful timer class for browser and node.js.",
  "main": "dist/duck-timer.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "webpack --hide-modules",
    "watch": "npm run build -- --watch"
  },
  "keywords": [
    "timer",
    "countdown"
  ],
  "author": "archco",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/duck-timer.git"
  },
  "bugs": {
    "url": "https://github.com/archco/duck-timer/issues"
  },
  "homepage": "https://github.com/archco/duck-timer#readme",
  "dependencies": {
    "eventemitter3": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ]
  }
}
